<Window
	x:Class="II_Avalonia.Main"
	xmlns="https://github.com/avaloniaui"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Name="wdwMain"
	Title="Main Window"
	Width="950"
	Height="1000"
	d:DesignHeight="1000"
	d:DesignWidth="950"
	Activated="OnActivated"
	Closed="OnClosed"
	Icon="avares://Infirmary Integrated/Resources/Icon_II.ico"
	LayoutUpdated="OnLayoutUpdated"
	mc:Ignorable="d">

  <Window.Styles>
    <Style Selector="Label">
      <Setter Property="VerticalContentAlignment" Value="Center" />
    </Style>

    <Style Selector="HeaderedContentControl.groupbox">
      <Setter Property="Template">
        <Setter.Value>
          <ControlTemplate>
            <Grid Margin="5">
              <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="*" />
              </Grid.RowDefinitions>
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
              </Grid.ColumnDefinitions>
              <Border
								Margin="5,0,0,0"
								Padding="5,0,5,0"
								Background="{TemplateBinding Background}"
								ZIndex="1">
                <TextBlock
									Background="Transparent"
									Text="{TemplateBinding Header}"
									TextBlock.FontWeight="Bold" />
              </Border>
              <Border
								Grid.RowSpan="2"
								Grid.ColumnSpan="2"
								Margin="0,10,0,0"
								BorderBrush="Black"
								BorderThickness="1"
								CornerRadius="3">
                <ContentPresenter Content="{TemplateBinding Content}" />
              </Border>
            </Grid>
          </ControlTemplate>
        </Setter.Value>
      </Setter>
    </Style>

    <Style Selector="Button.scenario">
      <Setter Property="HorizontalAlignment" Value="Stretch" />
      <Setter Property="Background" Value="White" />
      <Setter Property="Margin" Value="2" />
      <Setter Property="Padding" Value="10,2" />
    </Style>

    <Style Selector="Image.scenario">
      <Setter Property="Margin" Value="2" />
      <Setter Property="Width" Value="24" />
      <Setter Property="Height" Value="24" />
    </Style>

    <Style Selector="Button.device">
      <Setter Property="HorizontalAlignment" Value="Stretch" />
      <Setter Property="Background" Value="White" />
      <Setter Property="HorizontalContentAlignment" Value="Left" />
      <Setter Property="Margin" Value="2" />
      <Setter Property="Padding" Value="10,2" />
    </Style>

    <Style Selector="Image.device">
      <Setter Property="Margin" Value="2" />
      <Setter Property="Width" Value="48" />
      <Setter Property="Height" Value="48" />
    </Style>

    <Style Selector="Label.device">
      <Setter Property="Margin" Value="2" />
      <Setter Property="VerticalAlignment" Value="Center" />
      <Setter Property="FontSize" Value="16" />
    </Style>

    <Style Selector="Border.expander">
      <Setter Property="BorderBrush" Value="Black" />
      <Setter Property="BorderThickness" Value="1" />
      <Setter Property="CornerRadius" Value="3" />
    </Style>

    <Style Selector="Label.expander">
      <Setter Property="Margin" Value="2" />
      <Setter Property="FontSize" Value="13" />
      <Setter Property="FontWeight" Value="Bold" />
    </Style>

    <Style Selector="NumericUpDown.vitals">
      <Setter Property="MinWidth" Value="60" />
      <Setter Property="Height" Value="20" />
      <Setter Property="ClipValueToMinMax" Value="True" />
    </Style>

    <Style Selector="Label.divider">
      <Setter Property="HorizontalContentAlignment" Value="Center" />
    </Style>

    <Style Selector="Label.wavepart">
      <Setter Property="HorizontalContentAlignment" Value="Right" />
    </Style>

    <Style Selector="NumericUpDown.wavepart">
      <Setter Property="Increment" Value="0.1" />
      <Setter Property="Maximum" Value="1.0" />
      <Setter Property="Minimum" Value="-1.0" />
      <Setter Property="FormatString" Value="{}{0:0.0}" />
      <Setter Property="ShowButtonSpinner" Value="True" />
    </Style>

    <Style Selector="ComboBox.vitals">
      <Setter Property="Margin" Value="0,1,0,4" />
      <Setter Property="MinWidth" Value="100" />
      <Setter Property="HorizontalAlignment" Value="Stretch" />
    </Style>

    <Style Selector="Button.apply">
      <Setter Property="HorizontalAlignment" Value="Stretch" />
      <Setter Property="Background" Value="White" />
      <Setter Property="Margin" Value="2" />
      <Setter Property="Padding" Value="10,2" />
      <Setter Property="BorderBrush" Value="Black" />
      <Setter Property="BorderThickness" Value="1" />
    </Style>

    <Style Selector="Image.apply">
      <Setter Property="Margin" Value="2" />
      <Setter Property="Width" Value="24" />
      <Setter Property="Height" Value="24" />
    </Style>

    <Style Selector="Label.apply">
      <Setter Property="Margin" Value="2" />
      <Setter Property="VerticalAlignment" Value="Center" />
      <Setter Property="FontSize" Value="14" />
    </Style>
  </Window.Styles>

  <DockPanel LastChildFill="True">
    <!--  Main Menu  -->
    <Grid DockPanel.Dock="Top">
      <Grid.RowDefinitions>
        <RowDefinition Height="*" />
      </Grid.RowDefinitions>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>
      <Menu Grid.Column="0">
        <MenuItem Name="menuFile">
          <MenuItem Name="menuNew" Click="MenuNewSimulation_Click" />
          <Separator />
          <MenuItem Name="menuLoad" Click="MenuLoadFile_Click" />
          <MenuItem Name="menuSave" Click="MenuSaveFile_Click" />
          <Separator />
          <MenuItem Name="menuExit" Click="MenuExit_Click" />
        </MenuItem>

        <MenuItem Name="menuMirror">
          <MenuItem Name="menuMirrorStatus" />
          <Separator />
          <MenuItem Name="menuMirrorDeactivate" Click="MenuMirrorDeactivate_Click" />
          <Separator />
          <MenuItem Name="menuMirrorReceive" Click="MenuMirrorReceive_Click" />
          <MenuItem Name="menuMirrorBroadcast" Click="MenuMirrorBroadcast_Click" />
        </MenuItem>

        <MenuItem Name="menuSettings">
          <MenuItem Name="menuSetLanguage" Click="MenuSetLanguage_Click" />
        </MenuItem>

        <MenuItem Name="menuHelp">
          <MenuItem Name="menuCheckUpdate" Click="MenuCheckUpdate_Click" />
          <Separator />
          <MenuItem Name="menuAbout" Click="MenuAbout_Click" />
        </MenuItem>
      </Menu>

      <Menu
				Grid.Column="1"
				Margin="10,0"
				HorizontalAlignment="Right">
        <MenuItem
					Name="menuUpdate"
					Background="Yellow"
					Click="MenuUpdate_Click"
					IsVisible="False" />
      </Menu>
    </Grid>

    <Grid>
      <Grid.RowDefinitions>
        <RowDefinition Height="*" />
      </Grid.RowDefinitions>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="350" />
        <ColumnDefinition Width="*" />
      </Grid.ColumnDefinitions>

      <!--  Column for Devices (opening Cardiac Monitor, Ventilator, etc.  -->
      <ScrollViewer
				Grid.Row="0"
				Grid.Column="0"
				VerticalAlignment="Top"
				HorizontalScrollBarVisibility="Auto"
				VerticalScrollBarVisibility="Visible">

        <HeaderedContentControl
					Name="lblGroupDevices"
					Background="White"
					Classes="groupbox"
					Header="Header">
          <StackPanel Margin="3">
            <Button
							Name="btnDeviceMonitor"
							Classes="device"
							Click="ButtonDeviceMonitor_Click">
              <StackPanel Orientation="Horizontal">
                <Image Classes="device" Source="avares://Infirmary Integrated/Third_Party/Icon_DeviceMonitor_48.png" />
                <Label Name="lblDeviceMonitor" Classes="device" />
              </StackPanel>
            </Button>

            <Button
							Name="btnDeviceDefibrillator"
							Classes="device"
							Click="ButtonDeviceDefib_Click">
              <StackPanel Orientation="Horizontal">
                <Image Classes="device" Source="avares://Infirmary Integrated/Third_Party/Icon_DeviceDefibrillator_48.png" />
                <Label Name="lblDeviceDefibrillator" Classes="device" />
              </StackPanel>
            </Button>

            <Button
							Name="btnDevice12LeadECG"
							Classes="device"
							Click="ButtonDeviceECG_Click">
              <StackPanel Orientation="Horizontal">
                <Image Classes="device" Source="avares://Infirmary Integrated/Third_Party/Icon_Device12LeadECG_48.png" />
                <Label Name="lblDevice12LeadECG" Classes="device" />
              </StackPanel>
            </Button>

            <Button
							Name="btnDeviceIABP"
							Classes="device"
							Click="ButtonDeviceIABP_Click">
              <StackPanel Orientation="Horizontal">
                <Image Classes="device" Source="avares://Infirmary Integrated/Third_Party/Icon_DeviceIABP_48.png" />
                <Label Name="lblDeviceIABP" Classes="device" />
              </StackPanel>
            </Button>

            <Button
							Name="btnDeviceEFM"
							Classes="device"
							Click="ButtonDeviceEFM_Click"
							IsVisible="False">
              <StackPanel Orientation="Horizontal">
                <Image Classes="device" Source="avares://Infirmary Integrated/Third_Party/Icon_DeviceEFM_48.png" />
                <Label Name="lblDeviceEFM" Classes="device" />
              </StackPanel>
            </Button>
            <!-- <Button Name="btnDeviceVentilator" /> -->
            <!-- <Button Name="btnDeviceIVPump" /> -->
            <!-- <Button Name="btnDeviceLabResults" /> -->
          </StackPanel>
        </HeaderedContentControl>
      </ScrollViewer>

      <!--  Column for Patient Editor  -->
      <ScrollViewer
				Grid.Row="0"
				Grid.Column="1"
				HorizontalScrollBarVisibility="Auto"
				VerticalScrollBarVisibility="Visible">

        <Border
					Name="brdPendingChangesIndicator"
					Margin="5,0"
					BorderBrush="Transparent"
					BorderThickness="0,0,5,0">

          <StackPanel>

            <!--  Group for Scenario Player  -->
            <Border
							Name="brdScenarioPlayer"
							Margin="5,16,5,5"
							Classes="expander"
							IsVisible="False">
              <Expander
								Name="expScenarioPlayer"
								IsEnabled="False"
								IsExpanded="False">
                <Expander.Header>
                  <Label Name="lblGroupScenarioPlayer" Classes="expander" />
                </Expander.Header>
                <Grid Margin="3" HorizontalAlignment="Stretch">
                  <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                  </Grid.RowDefinitions>
                  <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition />
                  </Grid.ColumnDefinitions>

                  <Button
										Name="btnPreviousStep"
										Grid.Row="0"
										Grid.Column="0"
										Classes="scenario"
										Click="ButtonPreviousStep_Click">
                    <Image Classes="scenario" Source="avares://Infirmary Integrated/Third_Party/Icon_PlayerArrowBack_48.png" />
                  </Button>
                  <Button
										Name="btnPauseStep"
										Grid.Row="0"
										Grid.Column="1"
										Classes="scenario"
										Click="ButtonPauseStep_Click"
										IsEnabled="False">
                    <Image Classes="scenario" Source="avares://Infirmary Integrated/Third_Party/Icon_PlayerPause_48.png" />
                  </Button>
                  <Button
										Name="btnPlayStep"
										Grid.Row="0"
										Grid.Column="2"
										Classes="scenario"
										Click="ButtonPlayStep_Click"
										IsEnabled="False">
                    <Image Classes="scenario" Source="avares://Infirmary Integrated/Third_Party/Icon_PlayerArrowPlay_48.png" />
                  </Button>
                  <Button
										Name="btnNextStep"
										Grid.Row="0"
										Grid.Column="3"
										Classes="scenario"
										Click="ButtonNextStep_Click">
                    <Image Classes="scenario" Source="avares://Infirmary Integrated/Third_Party/Icon_PlayerArrowNext_48.png" />
                  </Button>

                  <Label
										Name="lblScenarioStep"
										Grid.Row="1"
										Grid.Column="0"
										Grid.ColumnSpan="4"
										HorizontalAlignment="Center" />

                  <Label
										Name="lblTimerStep"
										Grid.Row="2"
										Grid.Column="0"
										Grid.ColumnSpan="4"
										HorizontalAlignment="Center" />

                  <HeaderedContentControl
										Name="lblProgressionOptions"
										Grid.Row="3"
										Grid.Column="0"
										Grid.ColumnSpan="4"
										Margin="5,2"
										Background="#f2f2f2"
										Classes="groupbox">

                    <StackPanel Name="stackProgressions" Orientation="Vertical" />
                  </HeaderedContentControl>
                </Grid>
              </Expander>
            </Border>

            <!--  Group for Basic Vital Signs  -->
            <Border Margin="5,16,5,5" Classes="expander">
              <Expander
								Name="expVitalSigns"
								IsEnabled="True"
								IsExpanded="True">
                <Expander.Header>
                  <Label Name="lblGroupVitalSigns" Classes="expander" />
                </Expander.Header>
                <Grid Margin="3" HorizontalAlignment="Stretch">
                  <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                  </Grid.RowDefinitions>
                  <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition Width="30" />
                    <ColumnDefinition />
                  </Grid.ColumnDefinitions>
                  <Label
										Name="lblHR"
										Grid.Row="0"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numHR"
										Grid.Row="0"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="5"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="500"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblNIBP"
										Grid.Row="1"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numNSBP"
										Grid.Row="1"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="5"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="300"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblNDBP"
										Grid.Row="1"
										Grid.Column="2"
										Classes="divider"
										Content="/" />
                  <NumericUpDown
										Name="numNDBP"
										Grid.Row="1"
										Grid.Column="3"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="5"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="200"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblRR"
										Grid.Row="2"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numRR"
										Grid.Row="2"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="2"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="100"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblSPO2"
										Grid.Row="3"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numSPO2"
										Grid.Row="3"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="2"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="100"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblT"
										Grid.Row="4"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numT"
										Grid.Row="4"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0.0}"
										Increment="0.2"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="100"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />

                  <!--  Cardiac Rhythms  -->
                  <Label
										Name="lblCardiacRhythm"
										Grid.Row="5"
										Grid.Column="0" />
                  <ComboBox
										Name="comboCardiacRhythm"
										Grid.Row="6"
										Grid.Column="0"
										Grid.ColumnSpan="4"
										Classes="vitals"
										SelectionChanged="OnCardiacRhythm_Selected" />

                  <!--  Respiratory Rhythms  -->
                  <Label
										Name="lblRespiratoryRhythm"
										Grid.Row="7"
										Grid.Column="0"
										Grid.ColumnSpan="4" />
                  <ComboBox
										Name="comboRespiratoryRhythm"
										Grid.Row="8"
										Grid.Column="0"
										Grid.ColumnSpan="4"
										Classes="vitals"
										SelectionChanged="OnRespiratoryRhythm_Selected" />

                  <CheckBox
										Name="checkDefaultVitals"
										Grid.Row="9"
										Grid.Column="0"
										Grid.ColumnSpan="4"
										IsChecked="True" />
                </Grid>
              </Expander>
            </Border>

            <!--  Group for Advanced Hemodynamics  -->
            <Border Margin="5,16,5,5" Classes="expander">
              <Expander
								Name="expHemodynamics"
								IsEnabled="True"
								IsExpanded="False">
                <Expander.Header>
                  <Label Name="lblGroupHemodynamics" Classes="expander" />
                </Expander.Header>
                <Grid Margin="3" HorizontalAlignment="Stretch">
                  <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                    <RowDefinition />
                  </Grid.RowDefinitions>
                  <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition />
                    <ColumnDefinition Width="30" />
                    <ColumnDefinition />
                  </Grid.ColumnDefinitions>

                  <Label
										Name="lblETCO2"
										Grid.Row="0"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numETCO2"
										Grid.Row="0"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="2"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="100"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblCVP"
										Grid.Row="1"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numCVP"
										Grid.Row="1"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="1"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="100"
										Minimum="-100"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblASBP"
										Grid.Row="2"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numASBP"
										Grid.Row="2"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="5"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="300"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblADBP"
										Grid.Row="2"
										Grid.Column="2"
										Classes="divider"
										Content="/" />
                  <NumericUpDown
										Name="numADBP"
										Grid.Row="2"
										Grid.Column="3"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="5"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="200"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />

                  <Label
										Name="lblCO"
										Grid.Row="3"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numCO"
										Grid.Row="3"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0.0}"
										Increment="0.1 "
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="20"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />

                  <!--  Pulmonary Artery Catheter Rhythms  -->
                  <Label
										Name="lblPACatheterPlacement"
										Grid.Row="4"
										Grid.Column="0"
										Grid.ColumnSpan="4" />
                  <ComboBox
										Name="comboPACatheterPlacement"
										Grid.Row="5"
										Grid.Column="0"
										Grid.ColumnSpan="4"
										Classes="vitals"
										SelectionChanged="OnPulmonaryArteryRhythm_Selected" />

                  <Label
										Name="lblPSP"
										Grid.Row="6"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numPSP"
										Grid.Row="6"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="2"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="200"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblPDP"
										Grid.Row="6"
										Grid.Column="2"
										Classes="divider"
										Content="/" />
                  <NumericUpDown
										Name="numPDP"
										Grid.Row="6"
										Grid.Column="3"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="2"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="200"
										Minimum="0"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblICP"
										Grid.Row="7"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numICP"
										Grid.Row="7"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="1"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="50"
										Minimum="-50"
										ValueChanged="OnUIPatientParameter_Changed" />
                  <Label
										Name="lblIAP"
										Grid.Row="8"
										Grid.Column="0" />
                  <NumericUpDown
										Name="numIAP"
										Grid.Row="8"
										Grid.Column="1"
										Classes="vitals"
										FormatString="{}{0:#0}"
										Increment="1"
										KeyDown="OnUIPatientParameter_KeyDown"
										LostFocus="OnUIPatientParameter_LostFocus"
										Maximum="50"
										Minimum="-50"
										ValueChanged="OnUIPatientParameter_Changed" />
                </Grid>
              </Expander>
            </Border>

            <!--  Group for Respiratory Profile  -->
            <Border Margin="5,16,5,5" Classes="expander">
              <Expander
								Name="expRespiratoryProfile"
								IsEnabled="True"
								IsExpanded="False">
                <Expander.Header>
                  <Label Name="lblGroupRespiratoryProfile" Classes="expander" />
                </Expander.Header>
                <Grid Margin="3">
                  <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
