									LostFocus="OnUIPatientParameter_LostFocus"
									Maximum="500"
									Minimum="0"
									Style="{StaticResource intUpDown}" />

								<Label
									Name="lblFHRRhythms"
									Grid.Row="1"
									Grid.Column="0" />
								<ListBox
									Name="listFHRRhythms"
									Grid.Row="2"
									Grid.Column="0"
									Grid.ColumnSpan="2"
									GotFocus="OnUIPatientParameter_GotFocus"
									KeyDown="OnUIPatientParameter_KeyDown"
									LostFocus="OnUIPatientParameter_LostFocus"
									SelectionMode="Multiple" />

								<Label
									Name="lblFHRVariability"
									Grid.Row="3"
									Grid.Column="0" />
								<ComboBox
									Name="comboFHRVariability"
									Grid.Row="4"
									Grid.Column="0"
									Grid.ColumnSpan="2"
									GotFocus="OnUIPatientParameter_GotFocus"
									KeyDown="OnUIPatientParameter_KeyDown"
									LostFocus="OnUIPatientParameter_LostFocus" />

								<Label
									Name="lblUCFrequency"
									Grid.Row="5"
									Grid.Column="0" />
								<xctk:DecimalUpDown
									Name="numUCFrequency"
									Grid.Row="5"
									Grid.Column="1"
									GotFocus="OnUIPatientParameter_GotFocus"
									Increment="0.1"
									KeyDown="OnUIPatientParameter_KeyDown"
									LostFocus="OnUIPatientParameter_LostFocus"
									Maximum="60"
									Minimum="0"
									Style="{StaticResource decUpDown}" />
								<Label
									Name="lblUCDuration"
									Grid.Row="6"
									Grid.Column="0" />
								<xctk:IntegerUpDown
									Name="numUCDuration"
									Grid.Row="6"
									Grid.Column="1"
									GotFocus="OnUIPatientParameter_GotFocus"
									Increment="10"
									KeyDown="OnUIPatientParameter_KeyDown"
									LostFocus="OnUIPatientParameter_LostFocus"
									Maximum="600"
									Minimum="0"
									Style="{StaticResource intUpDown}" />
								<Label
									Name="lblUCIntensity"
									Grid.Row="7"
									Grid.Column="0" />
								<ComboBox
									Name="comboUCIntensity"
									Grid.Row="8"
									Grid.Column="0"
									Grid.ColumnSpan="2"
									GotFocus="OnUIPatientParameter_GotFocus"
									KeyDown="OnUIPatientParameter_KeyDown"
									LostFocus="OnUIPatientParameter_LostFocus" />
							</Grid>
						</Expander>
					</Border>

					<!--  Apply and Reset Buttons  -->
					<CheckBox
						Name="chkAutoApplyChanges"
						Margin="8"
						HorizontalAlignment="Center"
						Checked="OnAutoApplyChanges_Changed"
						Unchecked="OnAutoApplyChanges_Changed" />
					<StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
						<Button
							Name="btnParametersReset"
							Click="ButtonResetParameters_Click"
							Style="{StaticResource btn}">
							<StackPanel Orientation="Horizontal">
								<Image Source="{StaticResource Icon_Cross_32}" Style="{StaticResource btnImage}" />
								<Label Name="lblParametersReset" Style="{StaticResource btnLabel}" />
							</StackPanel>
						</Button>
						<Button
							Name="btnParametersApply"
							Click="ButtonApplyParameters_Click"
							Style="{StaticResource btn}">
							<StackPanel Orientation="Horizontal">
								<Label Name="lblParametersApply" Style="{StaticResource btnLabel}" />
								<Image Source="{StaticResource Icon_Check_32}" Style="{StaticResource btnImage}" />
							</StackPanel>
						</Button>
					</StackPanel>
				</StackPanel>
			</Border>
		</ScrollViewer>
	</DockPanel>
</Window>