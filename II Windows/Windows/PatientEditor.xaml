							   Grid.Row="3" />
							<ComboBox Name="comboFHRVariability"
								  Grid.Column="0"
								  Grid.Row="4"
								  Grid.ColumnSpan="2"
								KeyDown="OnUIPatientParameter_KeyDown"
								GotFocus="OnUIPatientParameter_GotFocus"
								LostFocus="OnUIPatientParameter_LostFocus" />

							<Label Name="lblUCFrequency"
							   Grid.Column="0"
							   Grid.Row="5" />
							<xctk:IntegerUpDown Name="numUCFrequency"
											Increment="5"
											Minimum="0"
											Maximum="3600"
											Grid.Column="1"
											Grid.Row="5"
											Style="{StaticResource intUpDown}"
											KeyDown="OnUIPatientParameter_KeyDown"
											GotFocus="OnUIPatientParameter_GotFocus"
											LostFocus="OnUIPatientParameter_LostFocus" />
							<Label Name="lblUCDuration"
							   Grid.Column="0"
							   Grid.Row="6" />
							<xctk:IntegerUpDown Name="numUCDuration"
											Increment="5"
											Minimum="0"
											Maximum="300"
											Grid.Column="1"
											Grid.Row="6"
											Style="{StaticResource intUpDown}"
											KeyDown="OnUIPatientParameter_KeyDown"
											GotFocus="OnUIPatientParameter_GotFocus"
											LostFocus="OnUIPatientParameter_LostFocus" />
							<Label Name="lblUCIntensity"
							   Grid.Column="0"
							   Grid.Row="7" />
							<ComboBox Name="comboUCIntensity"
								  Grid.Column="0"
								  Grid.Row="8"
								  Grid.ColumnSpan="2"
								KeyDown="OnUIPatientParameter_KeyDown"
								GotFocus="OnUIPatientParameter_GotFocus"
								LostFocus="OnUIPatientParameter_LostFocus" />
						</Grid>
					</Expander>
					<!-- </Border> -->

					<!-- Apply and Reset Buttons -->
					<CheckBox Name="chkAutoApplyChanges"
					HorizontalAlignment="Center"
						  Margin="8"
						  Checked="OnAutoApplyChanges_Changed"
						  Unchecked="OnAutoApplyChanges_Changed" />
					<StackPanel Orientation="Horizontal"
							HorizontalAlignment="Center">
						<Button Name="btnParametersReset"
							Click="ButtonResetParameters_Click"
							Style="{StaticResource btn}">
							<StackPanel Orientation="Horizontal">
								<Image Source="{StaticResource Icon_Cross_32}"
								   Style="{StaticResource btnImage}" />
								<Label Name="lblParametersReset"
								   Style="{StaticResource btnLabel}" />
							</StackPanel>
						</Button>
						<Button Name="btnParametersApply"
							Click="ButtonApplyParameters_Click"
							Style="{StaticResource btn}">
							<StackPanel Orientation="Horizontal">
								<Label Name="lblParametersApply"
								   Style="{StaticResource btnLabel}" />
								<Image Source="{StaticResource Icon_Check_32}"
								   Style="{StaticResource btnImage}" />
							</StackPanel>
						</Button>
					</StackPanel>
				</StackPanel>
			</Border>
		</ScrollViewer>
	</DockPanel>
</Window>