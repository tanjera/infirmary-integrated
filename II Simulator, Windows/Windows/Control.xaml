													   Style="{StaticResource wavepart_lbl}"
													   Content="V2" />
												<xctk:DecimalUpDown Name="numTWE_V2"
																	Grid.Row="1"
																	Grid.Column="5"
																	Style="{StaticResource wavepart_dud}"
																	KeyDown="OnUIPhysiologyParameter_KeyDown"
																	LostFocus="OnUIPhysiologyParameter_LostFocus"
																	ValueChanged="OnUIPhysiologyParameter_Changed" />
												<Label Grid.Row="2"
													   Grid.Column="4"
													   Style="{StaticResource wavepart_lbl}"
													   Content="V3" />
												<xctk:DecimalUpDown Name="numTWE_V3"
																	Grid.Row="2"
																	Grid.Column="5"
																	Style="{StaticResource wavepart_dud}"
																	KeyDown="OnUIPhysiologyParameter_KeyDown"
																	LostFocus="OnUIPhysiologyParameter_LostFocus"
																	ValueChanged="OnUIPhysiologyParameter_Changed" />

												<Label Grid.Row="0"
													   Grid.Column="6"
													   Style="{StaticResource wavepart_lbl}"
													   Content="V4" />
												<xctk:DecimalUpDown Name="numTWE_V4"
																	Grid.Row="0"
																	Grid.Column="7"
																	Style="{StaticResource wavepart_dud}"
																	KeyDown="OnUIPhysiologyParameter_KeyDown"
																	LostFocus="OnUIPhysiologyParameter_LostFocus"
																	ValueChanged="OnUIPhysiologyParameter_Changed" />
												<Label Grid.Row="1"
													   Grid.Column="6"
													   Style="{StaticResource wavepart_lbl}"
													   Content="V5" />
												<xctk:DecimalUpDown Name="numTWE_V5"
																	Grid.Row="1"
																	Grid.Column="7"
																	Style="{StaticResource wavepart_dud}"
																	KeyDown="OnUIPhysiologyParameter_KeyDown"
																	LostFocus="OnUIPhysiologyParameter_LostFocus"
																	ValueChanged="OnUIPhysiologyParameter_Changed" />
												<Label Grid.Row="2"
													   Grid.Column="6"
													   Style="{StaticResource wavepart_lbl}"
													   Content="V6" />
												<xctk:DecimalUpDown Name="numTWE_V6"
																	Grid.Row="2"
																	Grid.Column="7"
																	Style="{StaticResource wavepart_dud}"
																	KeyDown="OnUIPhysiologyParameter_KeyDown"
																	LostFocus="OnUIPhysiologyParameter_LostFocus"
																	ValueChanged="OnUIPhysiologyParameter_Changed" />
											</Grid>
										</HeaderedContentControl>
									</Grid>
								</Expander>
							</Border>

							<!--  Group for Obstetric Profile  -->
							<Border Style="{StaticResource expander_brd}">
								<Expander Name="expObstetricProfile"
										  IsEnabled="True"
										  IsExpanded="False">
									<Expander.Header>
										<Label Name="lblGroupObstetricProfile"
											   Style="{StaticResource expander_lbl}" />
									</Expander.Header>
									<Grid Margin="3"
										  HorizontalAlignment="Stretch">
										<Grid.RowDefinitions>
											<RowDefinition />
											<RowDefinition />
											<RowDefinition />
											<RowDefinition />
											<RowDefinition />
											<RowDefinition />
											<RowDefinition />
											<RowDefinition />
										</Grid.RowDefinitions>
										<Grid.ColumnDefinitions>
											<ColumnDefinition />
											<ColumnDefinition />
										</Grid.ColumnDefinitions>

										<Label Name="lblFHR"
											   Grid.Row="0"
											   Grid.Column="0" />
										<xctk:IntegerUpDown Name="numFHR"
															Grid.Row="0"
															Grid.Column="1"
															Style="{StaticResource vitals_iud}"
															FormatString="{}{0:#0}"
															Increment="5"
															KeyDown="OnUIPhysiologyParameter_KeyDown"
															LostFocus="OnUIPhysiologyParameter_LostFocus"
															Maximum="500"
															Minimum="0"
															ValueChanged="OnUIPhysiologyParameter_Changed" />

										<Label Name="lblFHRRhythms"
											   Grid.Row="1"
											   Grid.Column="0" />
										<ComboBox Name="comboFHRRhythm"
												  Grid.Row="1"
												  Grid.Column="1"
												  Style="{StaticResource vitals_cmb}"
												  SelectionChanged="OnUIPhysiologyParameter_Changed" />

										<Label Name="lblFHRVariability"
											   Grid.Row="2"
											   Grid.Column="0" />
										<xctk:IntegerUpDown Name="numFHRVariability"
															Grid.Row="2"
															Grid.Column="1"
															Style="{StaticResource vitals_iud}"
															FormatString="{}{0:#0}"
															Increment="5"
															KeyDown="OnUIPhysiologyParameter_KeyDown"
															LostFocus="OnUIPhysiologyParameter_LostFocus"
															Maximum="80"
															Minimum="0"
															ValueChanged="OnUIPhysiologyParameter_Changed" />

										<Label Name="lblUCFrequency"
											   Grid.Row="3"
											   Grid.Column="0" />
										<xctk:DecimalUpDown Name="numUCFrequency"
															Grid.Row="3"
															Grid.Column="1"
															Style="{StaticResource vitals_dud}"
															FormatString="{}{0:#0}"
															Increment="30"
															KeyDown="OnUIPhysiologyParameter_KeyDown"
															LostFocus="OnUIPhysiologyParameter_LostFocus"
															Maximum="600"
															Minimum="0"
															ValueChanged="OnUIPhysiologyParameter_Changed" />

										<Label Name="lblUCDuration"
											   Grid.Row="4"
											   Grid.Column="0" />
										<xctk:IntegerUpDown Name="numUCDuration"
															Grid.Row="4"
															Grid.Column="1"
															Style="{StaticResource vitals_iud}"
															FormatString="{}{0:#0}"
															Increment="10"
															KeyDown="OnUIPhysiologyParameter_KeyDown"
															LostFocus="OnUIPhysiologyParameter_LostFocus"
															Maximum="600"
															Minimum="30"
															ValueChanged="OnUIPhysiologyParameter_Changed" />

										<Label Name="lblUCIntensity"
											   Grid.Row="5"
											   Grid.Column="0" />
										<xctk:IntegerUpDown Name="numUCIntensity"
															Grid.Row="5"
															Grid.Column="1"
															Style="{StaticResource vitals_iud}"
															FormatString="{}{0:#0}"
															Increment="5"
															KeyDown="OnUIPhysiologyParameter_KeyDown"
															LostFocus="OnUIPhysiologyParameter_LostFocus"
															Maximum="100"
															Minimum="0"
															ValueChanged="OnUIPhysiologyParameter_Changed" />

										<Label Name="lblUCResting"
											   Grid.Row="6"
											   Grid.Column="0" />
										<xctk:IntegerUpDown Name="numUCResting"
															Grid.Row="6"
															Grid.Column="1"
															Style="{StaticResource vitals_iud}"
															FormatString="{}{0:#0}"
															Increment="5"
															KeyDown="OnUIPhysiologyParameter_KeyDown"
															LostFocus="OnUIPhysiologyParameter_LostFocus"
															Maximum="100"
															Minimum="0"
															ValueChanged="OnUIPhysiologyParameter_Changed" />
									</Grid>
								</Expander>
							</Border>

							<!--  Apply and Reset Buttons  -->

							<Grid Margin="3,5"
								  HorizontalAlignment="Stretch">
								<Grid.RowDefinitions>
									<RowDefinition />
									<RowDefinition />
								</Grid.RowDefinitions>
								<Grid.ColumnDefinitions>
									<ColumnDefinition />
									<ColumnDefinition />
								</Grid.ColumnDefinitions>

								<Button Name="btnAutoApplyChanges"
										Grid.Column="0"
										Grid.ColumnSpan="2"
										Grid.Row="0"
										Style="{StaticResource apply_btn}"
										Click="OnAutoApplyChanges_Changed">
									<StackPanel HorizontalAlignment="Center"
												Orientation="Horizontal">

										<CheckBox Name="chkAutoApplyChanges"
												  VerticalAlignment="Center"
												  Margin="5,0" />
										<Label Name="lblAutoApplyChanges"
											   Style="{StaticResource apply_lbl}" />
									</StackPanel>
								</Button>

								<Button Name="btnParametersReset"
										Grid.Column="0"
										Grid.Row="1"
										Style="{StaticResource apply_btn}"
										Click="ButtonResetParameters_Click">
									<StackPanel HorizontalAlignment="Center"
												Orientation="Horizontal">
										<Image Source="/Third_Party/Icon_TrashBin_128.png"
											   Style="{StaticResource apply_img}" />
										<Label Name="lblParametersReset"
											   Style="{StaticResource apply_lbl}" />
									</StackPanel>
								</Button>
								<Button Name="btnParametersApply"
										Grid.Column="1"
										Grid.Row="1"
										Style="{StaticResource apply_btn}"
										Click="ButtonApplyParameters_Click">
									<StackPanel HorizontalAlignment="Center"
												Orientation="Horizontal">
										<Image Source="/Third_Party/Icon_PasteData_128.png"
											   Style="{StaticResource apply_img}" />
										<Label Name="lblParametersApply"
											   Style="{StaticResource apply_lbl}" />
									</StackPanel>
								</Button>
							</Grid>
						</StackPanel>
					</Border>
				</StackPanel>
			</ScrollViewer>
		</Grid>
	</DockPanel>
</Window>